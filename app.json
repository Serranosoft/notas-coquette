{
    "expo": {
        "name": "Notas Coquette",
        "slug": "notas-coquette",
        "version": "2.2.3",
        "orientation": "portrait",
        "icon": "./assets/icon-notes.png",
        "userInterfaceStyle": "light",
        "newArchEnabled": false,
        "splash": {
            "image": "./assets/splash-notes-coquette.png",
            "resizeMode": "contain",
            "backgroundColor": "#FACCD6"
        },
        "scheme": "notas-coquette",
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": false,
            "buildNumber": "8",
            "bundleIdentifier": "com.paulaymanu113.notascoquette",
            "infoPlist": {
                "ITSAppUsesNonExemptEncryption": false,
                "NSUserTrackingUsageDescription": "Este identificador se utilizar√° para mostrarte anuncios personalizados y mejorar la relevancia de la publicidad que recibes."
            }
        },
        "locales": {
            "en": "./languages/english.json",
            "ar": "./languages/arab.json",
            "de": "./languages/deutsch.json",
            "hi": "./languages/hindi.json",
            "fr": "./languages/french.json",
            "id": "./languages/indonesian.json",
            "pt": "./languages/portuguese.json",
            "ru": "./languages/russian.json",
            "pl": "./languages/polish.json",
            "vi": "./languages/vietnamese.json",
            "tr": "./languages/turkish.json",
            "it": "./languages/italian.json",
            "fa": "./languages/farsi.json"
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/icon-notes.png",
                "backgroundColor": "#FACCD6"
            },
            "versionCode": 48,
            "package": "com.paulaymanu113.notascoquette"
        },
        "web": {
            "favicon": "./assets/favicon.png"
        },
        "extra": {
            "eas": {
                "projectId": "c19b7487-3441-46c9-aaef-1b2d4f52c873"
            }
        },
        "plugins": [
            [
                "expo-font",
                {
                    "android": {
                        "fonts": [
                            {
                                "fontFamily": "Regular",
                                "fontDefinitions": [
                                    {
                                        "path": "./assets/fonts/AncizarSans-Regular.ttf",
                                        "weight": "400",
                                        "style": "normal"
                                    }
                                ]
                            },
                            {
                                "fontFamily": "Medium",
                                "fontDefinitions": [
                                    {
                                        "path": "./assets/fonts/AncizarSans-Medium.ttf",
                                        "weight": "500",
                                        "style": "normal"
                                    }
                                ]
                            },
                            {
                                "fontFamily": "Semibold",
                                "fontDefinitions": [
                                    {
                                        "path": "./assets/fonts/AncizarSans-Bold.ttf",
                                        "weight": "600",
                                        "style": "normal"
                                    }
                                ]
                            }
                        ]
                    },
                    "ios": {
                        "fonts": [
                            "./assets/fonts/AncizarSans-Regular.ttf",
                            "./assets/fonts/AncizarSans-Medium.ttf",
                            "./assets/fonts/AncizarSans-Bold.ttf"
                        ]
                    }
                }
            ],
            "expo-router",
            "expo-localization",
            [
                "react-native-google-mobile-ads",
                {
                    "androidAppId": "ca-app-pub-3738413299329691~3051293801",
                    "iosAppId": "ca-app-pub-3738413299329691~4908502313",
                    "delayAppMeasurementInit": true,
                    "userTrackingUsageDescription": "This identifier will be used to deliver personalized ads to you."
                }
            ],
            [
                "expo-build-properties",
                {
                    "android": {
                        "extraProguardRules": "-keep class com.google.android.gms.internal.consent_sdk.** { *; }"
                    }
                }
            ],
            [
                "expo-notifications",
                {
                    "icon": "./assets/notification.png"
                }
            ],
            [
                "expo-tracking-transparency",
                {
                    "userTrackingPermission": "This identifier will be used to deliver personalized ads to you."
                }
            ]
        ]
    }
}